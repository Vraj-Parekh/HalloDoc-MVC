@{
    ViewData["Title"] = "ProviderOnCall";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    string active = "#providers";
}


<div class="container">
    <div class="d-flex justify-content-between align-items-center mt-4 mb-4">
        <h4>MDs On Call</h4>

        <a class="btn btn-outline-info bg-white text-info" onclick="history.back()"> &lt; Back </a>
    </div>

    <div class="row justify-content-between align-items-center pb-2">
        <div class="col-md-3">
            <div class="form-floating">
                <select class="form-select" id="state">
                    <option selected value="">All Regions</option>
                </select>
                <label for="state" class="form-label">Search</label>
            </div>
        </div>
        <div class="col-auto">
            <button class="btn btn-info text-white me-2">Calender View</button>
            <button class="btn btn-info text-white me-2">Shifts for Review</button>
        </div>
    </div>

    <div class="bg-white rounded shadow p-3">
        <h5>MD's On Call</h5>
        <h5>Physicians Off Duty</h5>

        <div class="row">
           @*  @foreach (var phy in Model.physician)
            {
                <div class="col-4 mb-3">
                    @if (phy.Photo == null)
                    {
                        <img src="~/Photos/person.png" height="50" width="50" />
                    }
                    else
                    {
                        <img src="~/uploads/@phy.Photo" height="50" width="50" />
                    }
                    @phy.Firstname @phy.Lastname
                </div>
            } *@
        </div>
    </div>
</div>
<partial name="_ActiveTabPartial" model=active />