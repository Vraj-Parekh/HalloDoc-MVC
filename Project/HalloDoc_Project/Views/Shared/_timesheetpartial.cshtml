@model FinalizeTimesheetDTO<div class="table-responsive">    <input type="text" asp-for="isfinalize" id="isfinalize" value="@Model.isfinalize" hidden />    <input type="text" asp-for="selectedvalue" hidden />    <table class="table mt-4 text-center bg-white">        <thead class="bg-light">            <tr class="my-2">                <th scope="col">Date</th>                <th scope="col">On-Call Hours</th>                <th scope="col">Total Hours</th>                <th scope="col">Weekend/Holiday</th>                <th scope="col">Number of Housecalls</th>                <th scope="col">Number of Phonecalls</th>            </tr>        </thead>        <tbody>            @for (int i = 0; i < Model.phytimesheet.Count; i++)            {                <tr>                    <td><input type="text" asp-for="@Model.phytimesheet[i].date" hidden>@Model.phytimesheet[i].date</td>                    <td>@Model.phytimesheet[i].totalhours</td>                    <td><input asp-for="@Model.phytimesheet[i].totalhours" type="text" pattern="[0-9]{1,2}" /></td>                    <td><input asp-for="@Model.phytimesheet[i].holiday" type="checkbox" /></td>                    <td><input asp-for="@Model.phytimesheet[i].noofhousecalls" type="text" pattern="[0-9]{1,}" /></td>                    <td><input asp-for="@Model.phytimesheet[i].noofphoneconsult" type="text" pattern="[0-9]{1,}" /></td>                </tr>            }        </tbody>    </table></div>