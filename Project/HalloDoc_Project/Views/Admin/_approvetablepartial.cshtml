@model FinalizeTimesheetDTO;<div class="table-responsive">    <input type="text" asp-for="isfinalize" id="isfinalize" value="@Model.isfinalize" hidden />    <input type="text" asp-for="selectedvalue" hidden />    <table class="table mt-4 text-center bg-white">        <thead class="bg-light">            <tr class="my-2">                <th scope="col">Start Date</th>                <th scope="col">End Date</th>                <th scope="col">Status</th>                <th scope="col">Action</th>            </tr>        </thead>        <tbody>            @{                int arr_length = Model.phytimesheet.Count;            }            <tr>                <td>@Model.phytimesheet[0].date</td>                <td>@Model.phytimesheet[arr_length-1].date</td>                <td>Pending</td>                <td><a class="btn btn-outline-info" onclick="approvedchange()">Approve</a></td>            </tr>        </tbody>    </table></div>